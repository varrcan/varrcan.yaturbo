	"keywords": ["bitrix-module", "yandex", "yandex-turbo"],
	"type": "bitrix-d7-module",
	"license": "MIT",
	"authors": [
		{
			"name": "Sergey Voloshin (Varrcan)",
			"email": "git@varrcan.me"
		}
	],
	"require": {
		"composer/installers": "1.7.*",
		"webpractik/agent": "1.*",
		"guzzlehttp/guzzle": "6.*"
